{
  "name": "@iba-cast-gallery/db-migrator",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "migrate:run": "yarn typeorm migration:run -d dist/data-source.js",
    "migrate:revert": "yarn typeorm migration:revert -d dist/data-source.js",
    "migrate:show": "yarn typeorm migration:show -d dist/data-source.js"
  },
  "dependencies": {
    "@iba-cast-gallery/dao": "workspace:*",
    "dotenv": "^16.4.5",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.22"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
