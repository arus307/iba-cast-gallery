{
  "name": "@iba-cast-gallery/dao",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn typeorm -d dist/data-source-for-generate.js migration:generate",
    "migration:create": "yarn typeorm migration:create"
  },
  "dependencies": {
    "@iba-cast-gallery/types": "workspace:*",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.22"
  },
  "devDependencies": {
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2"
  }
}
